 onSubmit(event) {\n        event.preventDefault()\n        alert(JSON.stringify(this.form))\n      },\n      onReset(event) {\n        event.preventDefault()\n        // Reset our form values\n        this.form.email = ''\n        this.form.name = ''\n        this.form.food = null\n        this.form.checked = []\n        // Trick to reset/clear native browser form validation state\n        this.show = false\n        this.$nextTick(() => {\n          this.show = true\n        })\n      }\n    }\n};\n</script>\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}